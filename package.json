  },
  "devDependencies": {
    "css-loader": "^5.0.1",
    "expose-loader": "^1.0.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
